<?xml version="1.0"?>
<Container version="2">
  <Name>gb92-nicehash</Name>
  <Repository>gavbenedict/nicehash-docker</Repository>
  <Registry>https://hub.docker.com/r/gavbenedict/nicehash-docker</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://www.nicehash.com/</Project>
  <Overview>The NiceHash Docker Miner is a free docker image designed for data centers equipped with high-end GPUs, enabling efficient monetization of idle server time without disrupting core operations. NiceHash Docker Miner facilitates profit switching among multiple GPU-minable algorithms and offers payouts in Bitcoin.</Overview>
  <Category>Crypto:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/gb92/nicehash-unraid/main/images/nicehash_logo_small_light.png</Icon>
  <ExtraParams>--runtime=nvidia</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1739560394</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="NVIDIA_DRIVER_CAPABILITIES" Target="NVIDIA_DRIVER_CAPABILITIES" Default="all" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">all</Config>
  <Config Name="NVIDIA_VISIBLE_DEVICES" Target="NVIDIA_VISIBLE_DEVICES" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false" />
  <Config Name="MINING_ADDRESS" Target="MINING_ADDRESS" Default="" Mode="" Description="NiceHash account mining address" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="MINING_WORKER_NAME" Target="MINING_WORKER_NAME" Default="unraid-docker" Mode="" Description="Name of the worker to be displayed in rig manager" Type="Variable" Display="always" Required="false" Mask="false" />
</Container>